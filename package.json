{
  "name": "openai-cli",
  "version": "1.0.0",
  "description": "A command-line utility for interfacing with OpenAI's API",
  "main": "app.js",
  "scripts": {
    "start": "node app.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^5.2.0",
    "inquirer": "^9.1.5",
    "openai": "^3.2.1",
    "openai-api": "^1.3.1",
    "ora": "^6.3.0",
    "pkg": "^5.8.1",
    "prompt-sync": "^4.2.0",
    "yargs": "^17.7.1"
  },
  "pkg": {
    "scripts": "./app.js",
    "targets": [
      "node16-macos-x64",
      "node16-linux-x64",
      "node16-linux-arm64",
      "node16-alpine-x64",
      "node16-alpine-arm64",
      "node16-win-x64"
    ],
    "outputPath": "./"
  },
  "bin": {
    "myapp": "app.js"
  },
  "directories": {
    "src": "src"
  }
}
